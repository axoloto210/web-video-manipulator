{
  "name": "Web Video Manipulator",
  "description": "Enables you to rewind or fast-forward videos",
  "version": "1.1",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png", 
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "web_video_manipulator.html",
    "default_icon": "icon.png"
  },
    "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ]
}